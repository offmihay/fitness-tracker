{
  "expo": {
    "name": "fitness-tracker",
    "scheme": "fitness-tracker",
    "slug": "fitness-tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.offmihay.fitnesstracker",
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.1017780084148-f9qpidfc4si6f18uum2jqbpn5ne4hkk4"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.1017780084148-f9qpidfc4si6f18uum2jqbpn5ne4hkk4"
        }
      ],
      "expo-localization",
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "cb003851-fdf1-42d5-85a8-1b8129f1d36e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
