{
  "expo": {
    "name": "",
    "scheme": "",
    "slug": "",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/light.png",
    "userInterfaceStyle": "automatic",
    "ios": {
      "icon": {
        "light": "./assets/icons/light.png",
        "dark": "./assets/icons/dark.png",
        "tinted": "./assets/icons/tinted.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/adaptive-icon.png"
      }
    },
    "web": {},
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-secure-store",
      [
        "expo-calendar",
        {
          "calendarPermission": "$(PRODUCT_NAME) needs to access your calendar to automatically add event yo your calendar."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "$(PRODUCT_NAME) needs to access your photos to let you share them with other people.",
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "$(PRODUCT_NAME) is using your locations to show nearby tournaments."
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#FFFFFF",
          "image": "./assets/icons/splash-icon-light.png",
          "dark": {
            "image": "./assets/icons/splash-icon-dark.png",
            "backgroundColor": "#121212"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a22c91b9-3761-4c74-8b79-9447a2ec51fa"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "offmihay"
  }
}
